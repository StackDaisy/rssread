<?xml version="1.0" encoding="utf-8"?>


<RelativeLayout android:id="@+id/fuck"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="#ffffff">

	<TextView android:id="@+id/Rtitle" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:padding="6px"
		android:text="TextView" android:textSize="21sp" android:textColor="#666666"
		android:gravity="left" android:layout_alignParentTop="true"
		android:layout_centerHorizontal="true" android:layout_alignParentLeft="true">
	</TextView>
	
	<TextView android:id="@+id/widget30" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text=""
		android:background="#bbbbbb" android:textSize="1sp" android:textStyle="bold"
		android:gravity="left" android:layout_below="@+id/Rtitle">
	</TextView>
	
	<ScrollView android:id="@+id/sc" android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:background="#ffffff"
		android:layout_below="@+id/widget30">
	
		<TextView android:padding="6px" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text=""
			android:textSize="20sp" android:id="@+id/content"
			android:layout_weight="1.0" android:layout_below="@+id/widget30"
			android:textColor="#000000" />
	
	</ScrollView>

</RelativeLayout>
